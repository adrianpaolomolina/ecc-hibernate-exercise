<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.apm.core">

   <class name="Roles" table="Roles">
      <id name="roleID" column="Role_ID" type="long">
         <generator class="sequence">
         <param name="sequence">roles_role_id_seq</param>
         </generator>
      </id>
      <property name="role" column="Roles" type="string"/>

  </class>

</hibernate-mapping>
